{
  "expo": {
    "name": "qraven-mobile",
    "slug": "qraven-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#DBDEB9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dubjay.qravenmobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#DBDEB9"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cb99bac0-bb52-4c23-ba67-737c10fa13e5"
      }
    },
    "owner": "dubjay"
  }
}
